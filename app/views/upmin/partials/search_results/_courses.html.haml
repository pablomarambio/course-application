%table.table
  %thead
    %tr
      - query.klass.attributes.each do |attribute|
        %th
          = attribute
      %th
      %th
  - query.upmin_results.each do |result|
    %tr.upmin-model{class: result.color}
      - result.attributes.each do |attribute|
        %td
          = attribute.value
      %td
        = link_to "Edit", result.path, class: "btn btn-primary btn-xs"
      %td
        = link_to("Delete",result.path, method: :delete, class: "btn btn-xs btn-danger delete delete_course_#{result.id}", title: "Delete #{result.title}.", data: {confirm: "Are you sure?"})